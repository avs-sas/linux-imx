		vcc-ext-in-supply = <&reg1_vcc_ext_in>;
		status = "okay";

		port {
			alvium_1_out: endpoint {
				remote-endpoint = <&max96717f_csi1_in>;
				data-lanes = <1 2 3 4>;
				link-frequencies = /bits/ 64 <650000000>;
			};
		};
	};

	deserializer@28 {
		compatible = "maxim,max96716a";
		reg = <0x28>;

		ports {
			#address-cells = <1>;
			#size-cells = <0>;
			port@0 {
				reg = <0>;
				max96716_gmsl_1_in: endpoint {
					remote-endpoint = <&max96917f_gmsl_1_out>;
				};
			};

			port@1 {
				reg = <1>;
				max96716_csi1_out: endpoint {
					data-lanes = <3 4 1 2>;
					link-frequencies = /bits/ 64 <650000000>;
					remote-endpoint = <&mipi_csi_1_in>;
				};
			};
		};

		i2c-gate {
			#address-cells = <1>;
			#size-cells = <0>;

			serializer@40 {
				compatible = "maxim,max96717f";
				reg = <0x40>;
				gpio-controller;
				#gpio-cells = <2>;
				#clock-cells = <0>;

				ports {
					#address-cells = <1>;
					#size-cells = <0>;

					port@0 {
						reg = <0>;
						max96717f_csi1_in: endpoint {
							data-lanes = <1 2 3 4>;
							remote-endpoint = <&alvium_1_out>;
						};
					};

					port@1 {
						reg = <1>;
						max96917f_gmsl_1_out: endpoint {
							remote-endpoint = <&max96716_gmsl_1_in>;
						};
					};
				};
			};
		};
	};
};

